   "source": [
    "def missing_number(arr):\n",
    "    n = len(arr)\n",
    "    total_sum = (n+1)*(n+2)/2\n",
    "    arr_sum = sum(arr)\n",
    "    missing_num = total_sum-arr_sum\n",
    "    return missing_num\n",
    "    \n",
    "arr = [1,2,3,4,6]\n",
    "missing_num = missing_number(arr)"
   ]
