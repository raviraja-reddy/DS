    "def linear_search(x,arr):\n",
    "    for i in range(len(arr)):\n",
    "        if arr[i] == x:\n",
    "            return i\n",
    "    return -1\n",
    "arr = [10, 20, 80, 30, 60, 50,110, 100, 130, 170]\n",
    "x = 180\n",
    "index = linear_search(x,arr)\n",
    "if index == -1:\n",
    "    print(\"Element is not present in list\")\n",
    "else:\n",
    "    print(index)"
   ]
